    "css-once:debug": "sass src/assets/scss/theme/main.scss:src/debug.css",
    "css-once:debug1": "node -e \"require('fs').writeFileSync('src/debug1.css',require('sass-js').compile('src/assets/scss/theme/main.scss').css)\"",
    "css:debug": "sass src/assets/scss/theme/main.scss:src/debug.css --watch",
    "css:debug-min": "sass src/assets/scss/theme/main.scss:src/debug.min.css --watch --style=compressed",
    "css-once:debug2": "node -e \"require('fs').writeFileSync('src/debug2.css',require('sass').compile('src/assets/scss/theme/main.scss').css)\"",
    "css-once:debug2-min": "node -e \"require('fs').writeFileSync('src/debug2.css',require('sass').compile('src/assets/scss/theme/main.scss').css,{style:'compressed'})\""
